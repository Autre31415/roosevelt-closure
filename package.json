{
  "name": "roosevelt-closure",
  "description": "Google Closure compiler support for Roosevelt MVC web framework",
  "author": "Eric Newport <kethinov@gmail.com>",
  "version": "0.1.2",
  "homepage": "https://github.com/kethinov/roosevelt-closure",
  "license": "Creative Commons Attribution 4.0 International License",
  "main": "roosevelt-closure.js",
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=0.10.0"
  },
  "engineStrict": true,
  "dependencies": {
    "closurecompiler": "^1.0.0"
  },
  "jshintConfig": {
    "camelcase": true,
    "curly": true,
    "devel": true,
    "evil": true,
    "indent": 2,
    "node": true
  },
  "devDependencies ": {
    "jshint": "*"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kethinov/roosevelt-closure.git"
  },
  "keywords": [
    "roosevelt.js",
    "googleclosure",
    "node.js",
    "closure",
